<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
           http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">
   	<bean id="a1" class="cn.com.xalead.spring.Message">
   		<property name="name">
   			<value>zangwenyang</value>
   		</property>
   		<property name="children">
   			<list>
   				<value>1</value>
   				<value>2</value>
   			</list>
   		</property>
   		<property name="hm">
   			<map>
   				<entry key="zang" value="wenyang">
   				</entry>
   				<entry key="jin" value="ping">
   				</entry>   				
   					
   			</map>
   			
   			
   		</property>
   	</bean>  
   	<bean id="a2" class="cn.com.xalead.spring.NewMesssage"/> 
   	
   	<bean id="userManager" class="cn.com.xalead.spring.user.UserManager" scope="singleton" />
   	<bean id="userAction" class="cn.com.xalead.spring.user.UserAction" scope="singleton">   		
   		<property name="manager" ref="userManager">	  			
   		</property>
   	</bean>
   	
   	<bean id="constructDemo" class="cn.com.xalead.spring.ConstructDemo">
   		<constructor-arg  value="1" />
   		<constructor-arg   value="hello" />
   	</bean> 
   	  	  	
</beans>
